@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <http://tib.de/ontario/mapping#> .
@prefix iasis: <http://project-iasis.eu/vocab/> .

<#Gene_Meth>
	rml:logicalSource [ rml:source "/data/data/genomic/Preprocessed_MayJune/COSMIC_methylation_preprocessed.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Gene/{GENE_NAME}";
		rr:class iasis:Gene
	];
	rr:predicateObjectMap [
		rr:predicate iasis:label;
		rr:objectMap [
			rml:reference "GENE_NAME"
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:gene_isRelatedTo_sample;
		rr:objectMap [ 
			rr:parentTriplesMap <#Sample_Meth> 
		]
	].

<#Sample_Meth>
	rml:logicalSource [ rml:source "/data/data/genomic/Preprocessed_MayJune/COSMIC_methylation_preprocessed.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Sample/{ID_SAMPLE}";
		rr:class iasis:Sample
	];
	rr:predicateObjectMap [
		rr:predicate iasis:id;
		rr:objectMap [
			rml:reference "ID_SAMPLE"
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:label;
		rr:objectMap [
			rml:reference "SAMPLE_NAME"]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:primaryTissue;
		rr:objectMap [ 
			rml:reference "PRIMARY_SITE" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:tissueSubtype1;
		rr:objectMap [ 
			rml:reference "SITE_SUBTYPE_1"
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:primaryHistology;
		rr:objectMap [ 
			rml:reference "PRIMARY_HISTOLOGY" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:histologySubtype1;
		rr:objectMap [ 
			rml:reference "HISTOLOGY_SUBTYPE_1" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:sample_isTakenFrom_tumor;
		rr:objectMap [ 
			rr:parentTriplesMap <#Tumor_Meth> 
		]
	].

<#Tumor_Meth>
	rml:logicalSource [ rml:source "/data/data/genomic/Preprocessed_MayJune/COSMIC_methylation_preprocessed.csv";
						rml:referenceFormulation ql:CSV ];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Tumor/{ID_TUMOUR}";
		rr:class iasis:Tumor
	];
	rr:predicateObjectMap [
		rr:predicate iasis:id;
		rr:objectMap [ 
			rml:reference "ID_TUMOUR" 
		]
	].

<#CGI_Meth>
	rml:logicalSource [ rml:source "/data/data/genomic/Preprocessed_MayJune/COSMIC_methylation_preprocessed.csv";
						rml:referenceFormulation ql:CSV ];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/CGI/{FRAGMENT_ID}";
		rr:class iasis:CGI
	];
	rr:predicateObjectMap [
		rr:predicate iasis:id;
		rr:objectMap [ 
			rml:reference "FRAGMENT_ID"
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:methylation;
		rr:objectMap [ 
			rml:reference "Methylation"
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:beta_value;
		rr:objectMap [ 
			rml:reference "BETA_VALUE"
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:avg_beta_value;
		rr:objectMap [ 
			rml:reference "AVG_BETA_VALUE_NORMAL"
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:two_sided_p-value;
		rr:objectMap [ 
			rml:reference "TWO_SIDED_P_VALUE"
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:genomic_position;
		rr:objectMap [ 
			rml:reference "POSITION" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:cgi_chromosome;
		rr:objectMap [ 
			rml:reference "CHROMOSOME" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:cgi_study_id;
		rr:objectMap [ 
			rml:reference "STUDY_ID" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:cgi_isLocatedIn_tumor;
		rr:objectMap [ 
			rr:parentTriplesMap <#Tumor_Meth> 
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:cgi_isLocatedIn_sample;
		rr:objectMap [ 
			rr:parentTriplesMap <#Sample_Meth> 
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:cgi_isLocatedIn_gene;
		rr:objectMap [ 
			rr:parentTriplesMap <#Gene_Meth> 
		]
	].




