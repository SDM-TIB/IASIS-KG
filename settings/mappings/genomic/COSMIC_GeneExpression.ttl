@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <http://tib.de/ontario/mapping#> .
@prefix iasis: <http://project-iasis.eu/vocab/> .

<#Gene_GE>
	rml:logicalSource [ rml:source "/data/data/genomic/Preprocessed_MayJune/COSMIC_geneExpression_preprocessed.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Gene/{GENE_NAME}";
		rr:class iasis:Gene
	];
	rr:predicateObjectMap [
		rr:predicate iasis:label;
		rr:objectMap [
			rml:reference "GENE_NAME"
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:gene_isRelatedTo_sample;
		rr:objectMap [ 
			rr:parentTriplesMap <#Sample_GE> 
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:gene_isTranscribedAs_transcription;
		rr:objectMap [ 
			rr:parentTriplesMap <#Transcription_GE> 
		]
	].

<#Sample_GE>
	rml:logicalSource [ rml:source "/data/data/genomic/Preprocessed_MayJune/COSMIC_geneExpression_preprocessed.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Sample/{SAMPLE_ID}";
		rr:class iasis:Sample
	];
	rr:predicateObjectMap [
		rr:predicate iasis:id;
		rr:objectMap [
			rml:reference "SAMPLE_ID"
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:label;
		rr:objectMap [
			rml:reference "SAMPLE_NAME"
		]
	].

<#Gexp_GE>
	rml:logicalSource [ rml:source "/data/data/genomic/Preprocessed_MayJune/COSMIC_geneExpression_preprocessed.csv";
						rml:referenceFormulation ql:CSV ];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Gexp/{GENE_NAME}";
		rr:class iasis:Gexp
	];

	rr:predicateObjectMap [
		rr:predicate iasis:z_score;
		rr:objectMap [ 
			rml:reference "Z_SCORE"
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:label;
		rr:objectMap [ 
			rml:reference "GENE_NAME" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:regulation;
		rr:objectMap [ 
			rml:reference "REGULATION" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:gexp_study_id;
		rr:objectMap [ 
			rml:reference "ID_STUDY"
		]
	].

<#Transcription_GE>
	rml:logicalSource [ rml:source "/data/data/genomic/Preprocessed_MayJune/COSMIC_geneExpression_preprocessed.csv";
						rml:referenceFormulation ql:CSV ];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Transcription/{GENE_NAME}";
		rr:class iasis:Transcription
	];
	rr:predicateObjectMap [
		rr:predicate iasis:transcription_isExpressedAs_gexp;
		rr:objectMap [ 
			rr:parentTriplesMap <#Gexp_GE> 
		]
	].
