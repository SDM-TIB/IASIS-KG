@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <http://tib.de/ontario/mapping#> .
@prefix iasis: <http://project-iasis.eu/vocab/> .

<#Phenotype>
	rml:logicalSource [ rml:source "/home/jozashoori/External/Data/Deliverable/data/genomic/phenotype_rows_hpo.csv";
						rml:referenceFormulation ql:CSV
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Phenotype/{name}";
		rr:class iasis:Phenotype 	
	];
	rr:predicateObjectMap [
		rr:predicate iasis:phenotypeLabel;
		rr:objectMap [
			rml:reference "name"
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:hasCUIAnnotation;
		rr:objectMap [
			rr:parentTriplesMap <#Annotation1>
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:hasHPOAnnotation;
		rr:objectMap [
			rr:parentTriplesMap <#Annotation2>
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:hasHPOAnnotation;
		rr:objectMap [
			rr:parentTriplesMap <#Annotation3>
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:hasHPOAnnotation;
		rr:objectMap [
			rr:parentTriplesMap <#Annotation4>
		]
	].

<#Annotation1>
	rml:logicalSource [ rml:source "/home/jozashoori/External/Data/Deliverable/data/genomic/phenotype_rows_hpo.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Annotation/{cui}";
		rr:class iasis:Annotation
	];
	rr:predicateObjectMap [
		rr:predicate iasis:annID;
		rr:objectMap [ 
			rml:reference "cui" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:provenance;
		rr:objectMap [
			rr:template "http://project-iasis.eu/SourceName/UMLS"
		]
	].	

