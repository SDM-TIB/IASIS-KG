<#ADPatient>
	rml:logicalSource [ rml:source "/data/data/genomic/.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/ADPatient/{eid}";
		rr:class iasis:ADPatient
	];
	rr:predicateObjectMap [
		rr:predicate iasis:gender;
		rr:objectMap [
			rr:template "http://project-iasis.eu/Gender/{31}";
		]
	];	
	rr:predicateObjectMap [
		rr:predicate iasis:birthYear;
		rr:objectMap [
			rml:reference "34";
		]
	];
