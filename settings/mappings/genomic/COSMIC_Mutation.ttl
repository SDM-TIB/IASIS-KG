@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <http://tib.de/ontario/mapping#> .
@prefix iasis: <http://project-iasis.eu/vocab/> .

<#Gene_Mu>
	rml:logicalSource [ rml:source "/app/Data/COSMIC/Preprocessed_MayJune/COSMIC_mutation_preprocessed.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Gene/{Gene name}";
		rr:class iasis:Gene
	];
	rr:predicateObjectMap [
		rr:predicate iasis:label;
		rr:objectMap [
			rml:reference "Gene name";
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:id;
		rr:objectMap [
			rml:reference "HGNC ID";
		]
	];	
	rr:predicateObjectMap [
		rr:predicate iasis:cds_length;
		rr:objectMap [ 
			rml:reference "Gene CDS length"; 
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:gene_isRelatedTo_sample;
		rr:objectMap [ 
			rr:parentTriplesMap <#Sample_Mu> 
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:gene_isTranscribedAs_transcription;
		rr:objectMap [ 
			rr:template "http://project-iasis.eu/Transcription/{Gene name}";
		]
	].

