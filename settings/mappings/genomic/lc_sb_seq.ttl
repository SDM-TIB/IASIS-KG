<#Variation>
	rml:logicalSource [ rml:source "/data/data/genomic/LC_SB_Seq.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Variation/{Gene.refGene}_{Chr}_{Start}-{End}_{Ref}~{Alt}";
		rr:class iasis:Variation
	];
	rr:predicateObjectMap [
		rr:predicate owl:sameAs;
		rr:objectMap [
			rr:template "http://project-iasis.eu/Variation/{mFormat}";
		]
	];	
	rr:predicateObjectMap [
		rr:predicate owl:sameAs;
		rr:objectMap [
			rr:template "http://project-iasis.eu/Variation/{cFormat}";
		]
	];
