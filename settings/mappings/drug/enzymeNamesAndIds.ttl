@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <http://tib.de/ontario/mapping#> .
@prefix iasis: <http://project-iasis.eu/vocab/> .

<#Enzyme>
	rml:logicalSource [ rml:source "/app/Data/Drug/Drug-Enzyme/enzyme_names.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Enzyme/{EnzimeKEGGID}";
		rr:class iasis:Enzyme
	];
	rr:predicateObjectMap [
		rr:predicate iasis:label;
		rr:objectMap [
			rml:reference "EnzymeName";
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:id;
		rr:objectMap [
			rml:reference "EnzimeKEGGID";
		]
	].

