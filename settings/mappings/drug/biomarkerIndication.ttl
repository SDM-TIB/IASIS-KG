@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <http://tib.de/ontario/mapping#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix iasis: <http://project-iasis.eu/vocab/> .

<#Drug_Biomarker>
	rml:logicalSource [ rml:source "/data/data/drug/biomarkers_drugs.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Biomarker/{biomarker_CUI}";
		rr:class iasis:Biomarker
	];
	rr:predicateObjectMap [
		rr:predicate iasis:biomarkerLabel;
		rr:objectMap [ 
			rml:reference "biomarker" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:biomarkerCuiID;
		rr:objectMap [ 
			rml:reference "biomarker_CUI" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:hasIndication;
		rr:objectMap [
			rr:template "http://project-iasis.eu/Drug/{drugID}";
		]
	].


