 @prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <http://tib.de/ontario/mapping#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix iasis: <http://project-iasis.eu/vocab/> .

<#EffectInteraction>
	rml:logicalSource [ rml:source "/data/data/drug/wp4_drugs_drugs_interactions.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/EffectInteraction/{Drug1}_interactsWith_{Drug2}";
		rr:class iasis:EffectInteraction
	];
	rr:predicateObjectMap [
		rr:predicate iasis:affects;
		rr:objectMap [ 
			rr:template "http://project-iasis.eu/Drug/{Drug1}" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:isAffected;
		rr:objectMap [ 
			rr:template "http://project-iasis.eu/Drug/{Drug2}" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:provenance;
		rr:objectMap [
			rr:template "http://project-iasis.eu/SourceName/PubMed"
		]
	].
<#DDI>
	rml:logicalSource [ rml:source "/data/data/drug/wp4_drugs_drugs_interactions.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Drug/{Drug1}";
	];
	rr:predicateObjectMap [
		rr:predicate iasis:drugDrugInteraction_pubMed;
		rr:objectMap [
			rr:template "http://project-iasis.eu/Drug/{Drug2}";
		]
	].
