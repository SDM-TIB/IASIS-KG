@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <http://tib.de/ontario/mapping#> .
@prefix iasis: <http://project-iasis.eu/vocab/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

<#Drug_DTI>
	rml:logicalSource [ rml:source "/data/data/drug/DrugBankIDsAndNames_withoutDbpedia.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Drug/{DrugID}";
		rr:class iasis:Drug
	];
	rr:predicateObjectMap [
		rr:predicate iasis:label;
		rr:objectMap [
			rml:reference "DrugName"
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:id;
		rr:objectMap [
			rml:reference "DrugID"
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:externalLink;
		rr:objectMap [
			rr:template "https://www.drugbank.ca/drugs/{DrugID}"
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:sameAs;
		rr:objectMap [
			rr:template "http://bio2rdf.org/drugbank:{DrugID}"
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:sameAs;
		rr:objectMap [
			rr:template "http://wifo5-04.informatik.uni-mannheim.de/drugbank/page/drugs/{DrugID}"
		]
	].
