@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <http://tib.de/ontario/mapping#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix iasis: <http://project-iasis.eu/vocab/> .

<#EffectInteraction>
	rml:logicalSource [ rml:source "/data/data/drug/wp4_drug_drug_interactions_AD.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/EffectInteraction/{DRUGBANK_ID_1}_interactsWith_{DRUGBANK_ID_2}";
		rr:class iasis:EffectInteraction
	];
	rr:predicateObjectMap [
		rr:predicate iasis:affects;
		rr:objectMap [ 
			rr:template "http://project-iasis.eu/Drug/{DRUGBANK_ID_1}" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:isAffected;
		rr:objectMap [ 
			rr:template "http://project-iasis.eu/Drug/{DRUGBANK_ID_2}" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:confidence;
		rr:objectMap [ 
			rml:reference "Probability" 
		]
	];	
	rr:predicateObjectMap [
		rr:predicate iasis:provenance;
		rr:objectMap [
			rr:template "http://project-iasis.eu/SourceName/Demokritos"
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:interaction_isRelatedTo_Disease;
		rr:objectMap [ 
			rr:template "http://project-iasis.eu/Disorder/Alzheimer"; 
		]
	].		
