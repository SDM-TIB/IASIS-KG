@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <http://tib.de/ontario/mapping#> .
@prefix iasis: <http://project-iasis.eu/vocab/> .

<#Drug_DTI>
	rml:logicalSource [ rml:source "/data/data/Drug/drug_target_interactions.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Drug/{Drug accession number}";
		rr:class iasis:Drug
	];
	rr:predicateObjectMap [
		rr:predicate iasis:label;
		rr:objectMap [
			rml:reference "Drug name"
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:id;
		rr:objectMap [
			rml:reference "Drug accession number"
		]
	];

	rr:predicateObjectMap [
		rr:predicate iasis:drug_interactsWith_protein;
		rr:objectMap [ 
			rr:parentTriplesMap <#Protein_DTI> 
		]
	].

	
<#Protein_DTI>
	rml:logicalSource [ rml:source "/data/data/Drug/drug_target_interactions.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Protein/{Target ensembl id}";
		rr:class iasis:Protein
	];
	rr:predicateObjectMap [
		rr:predicate iasis:id;
		rr:objectMap [
			rml:reference "Target ensembl id"
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:label;
		rr:objectMap [
			rml:reference "Target name"
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:source;
		rr:objectMap [
			rr:constant "http://project-iasis.eu/SourceName/ENSEMBL"
		]
	].
