@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <http://tib.de/ontario/mapping#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix iasis: <http://project-iasis.eu/vocab/> .

<#Target>
	rml:logicalSource [ rml:source "/home/jozashoori/External/Data/Deliverable/data/drug/target.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Target/{Target_name}";
		rr:class iasis:Target
	];
	rr:predicateObjectMap [
		rr:predicate iasis:targetLabel;
		rr:objectMap [
			rml:reference "Target_name";
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:targetID;
		rr:objectMap [
			rml:reference "Target_ID";
		]
	];
	rr:predicateObjectMap [
		rr:predicate owl:sameAs;
		rr:objectMap [
			rr:template "http://project-iasis.eu/BioSystem/{Target_ID}";
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:uniprotID;
		rr:objectMap [
			rml:reference "Uni_ID";
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:externalLink;
		rr:objectMap [
			rr:template "https://www.uniprot.org/uniprot/{Uni_ID}";
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:target_isRelatedTo_gene;
		rr:objectMap [
			rr:template "http://project-iasis.eu/Gene/{Gene_name}";
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:hasAnnotation;
		rr:objectMap [
			rr:parentTriplesMap <#Annotation>
		]
	].

<#Annotation>
	rml:logicalSource [ rml:source "/home/jozashoori/External/Data/Deliverable/data/drug/target.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Annotation/{CUI}";
		rr:class iasis:Annotation
	];
	rr:predicateObjectMap [
		rr:predicate iasis:annID;
		rr:objectMap [ 
			rml:reference "CUI" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:provenance;
		rr:objectMap [
			rr:template "http://project-iasis.eu/SourceName/UMLS"
		]
	].	