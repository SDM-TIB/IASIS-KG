@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <http://tib.de/ontario/mapping#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix iasis: <http://project-iasis.eu/vocab/> .

<#DementiaPatient>
	rml:logicalSource [ rml:source "/data/data/dementia/patient_ad.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/DementiaPatient/{patient_id}";
	];
	rr:predicateObjectMap [
		rr:predicate owl:sameAs;;
		rr:objectMap [
			rr:parentTriplesMap <#ADPatient>
		]
	].
<#ADPatient>
	rml:logicalSource [ rml:source "/data/data/dementia/patient_ad.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/ADPatient/{patient_id}";
		rr:class iasis:ADPatient
	];
	rr:predicateObjectMap [
		rr:predicate iasis:diagnosisEpisodeStartDate;
		rr:objectMap [
			rml:reference "EPISODE_DATE_PREV";
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:diagnosisEpisodeEndDate;
		rr:objectMap [
			rml:reference "EPISODE_DATE";
		]
	];	
	rr:predicateObjectMap [
		rr:predicate iasis:diseaseConfidence;
		rr:objectMap [
			rml:reference "AD_STATUS";
		]
	].			