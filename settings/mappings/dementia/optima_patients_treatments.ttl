@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <http://tib.de/ontario/mapping#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix iasis: <http://project-iasis.eu/vocab/> .

<#ADPatient>
	rml:logicalSource [ rml:source "/data/data/dementia/optima_patients_treatments.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/ADPatient/{patient_id}";
	];
	rr:predicateObjectMap [
		rr:predicate iasis:hasDrug;
		rr:objectMap [ 
			rr:template "http://project-iasis.eu/Drug/{Medication_label}"
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:hasDiagnosisAge;
		rr:objectMap [ 
			rml:reference "Age_At_Episode" 
		]
	];	
	rr:predicateObjectMap [
		rr:predicate iasis:hasDiagnosisDate;
		rr:objectMap [ 
			rml:reference "EPISODE_DATE" 
		]
	].	

<#Drug1>
	rml:logicalSource [ rml:source "/data/data/dementia/optima_patients_treatments.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Drug/{Medication_label}";
		rr:class iasis:DementiaDrug
	];
	rr:predicateObjectMap [
		rr:predicate iasis:drugLabel;
		rr:objectMap [ 
			rml:reference "Medication_label" 
		]
	].

<#Drug2>
	rml:logicalSource [ rml:source "/data/data/dementia/optima_patients_treatments.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Drug/{Medication_label}";
		rr:class iasis:Drug;
	];
	rr:predicateObjectMap [
		rr:predicate iasis:drugLabel;
		rr:objectMap [ 
			rml:reference "Medication_label" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:hasCUIAnnotation;
		rr:objectMap [
			rr:parentTriplesMap <#Annotation>
		]
	].	

<#Annotation>
	rml:logicalSource [ rml:source "/data/data/dementia/optima_patients_treatments.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Annotation/{Medication_cui}";
		rr:class iasis:Annotation
	];
	rr:predicateObjectMap [
		rr:predicate iasis:annID;
		rr:objectMap [ 
			rml:reference "Medication_cui" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:provenance;
		rr:objectMap [
			rr:template "http://project-iasis.eu/SourceName/UMLS"
		]
	].
