@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <http://tib.de/ontario/mapping#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix iasis: <http://project-iasis.eu/vocab/> .

<#ADPatient>
	rml:logicalSource [ rml:source "/data/data/dementia/optima_patients_filtered.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/ADPatient/{patient_id}";
		rr:class iasis:ADPatient
	];
	rr:predicateObjectMap [
		rr:predicate iasis:hasExamination;
		rr:objectMap [ 
			rr:parentTriplesMap <#Examination>
		]
	].
<#Examination>
	rml:logicalSource [ rml:source "/data/data/dementia/optima_patients_filtered.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Examination/{patient_id}:{EPISODE_DATE}";
		rr:class iasis:Examination;
	];
	rr:predicateObjectMap [
		rr:predicate iasis:hasExaminationAge;
		rr:objectMap [ 
			rml:reference "Age_At_Episode" 
		]
	];	
	rr:predicateObjectMap [
		rr:predicate iasis:hasEpisodeDate;
		rr:objectMap [ 
			rml:reference "EPISODE_DATE" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:hasMMS_score1;
		rr:objectMap [ 
			rml:reference "MMS1" 
		]
	];	
	rr:predicateObjectMap [
		rr:predicate iasis:hasMMS_score2;
		rr:objectMap [ 
			rml:reference "MMS2" 
		]
	].	
