@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <http://tib.de/ontario/mapping#> .
@prefix iasis: <http://project-iasis.eu/vocab/> .

<#DrugSideEffectPrediction>
	rml:logicalSource [ rml:source "/data/data/drug_predictions/drug_sideeffect_prediction_names.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Drug/{DrugID}";
	];
	rr:predicateObjectMap [
		rr:predicate iasis:drug_isPredictedToHave_sideEffect;
		rr:objectMap [
			rr:template "http://project-iasis.eu/Phenotype/{name}";
		]
	].

