@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <http://tib.de/ontario/mapping#> .
@prefix iasis: <http://project-iasis.eu/vocab/> .

<#PredictedDrugSideEffect_PDSE>
	rml:logicalSource [ rml:source "/data/data/drug_predictions/drug_sideeffect_prediction_names.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/PredictedDrugSideEffect/{DrugID}_{name}";
		rr:class iasis:PredictedDrugSideEffect
	];
	rr:predicateObjectMap [
		rr:predicate iasis:provenance;
		rr:objectMap [
			rr:template "http://project-iasis.eu/SourceName/semEP"
		]
	].	

<#Drug_DSE>
	rml:logicalSource [ rml:source "/data/data/drug_predictions/drug_sideeffect_prediction_names.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Drug/{DrugID}"
	];
	rr:predicateObjectMap [
		rr:predicate iasis:drug_isRelatedTo_pdse;
		rr:objectMap [
			rr:parentTriplesMap <#PredictedDrugSideEffect_PDSE>
		]
	].

<#SideEffect_DSE>
	rml:logicalSource [ rml:source "/data/data/drug_predictions/drug_sideeffect_prediction_names.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Phenotype/{name}"
	];
	rr:predicateObjectMap [
		rr:predicate iasis:sideEffect_isRelatedTo_pdse;
		rr:objectMap [
			rr:parentTriplesMap <#PredictedDrugSideEffect_PDSE>
		]
	].	