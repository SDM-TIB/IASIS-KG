@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <http://tib.de/ontario/mapping#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix iasis: <http://project-iasis.eu/vocab/> .

<#InteractionPrediction>
	rml:logicalSource [ rml:source "/data/data/drug/DrugEnzyme_prediction_LUH.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/InteractionPrediction/{Drug}_interactsWith_{Enzyme}";
		rr:class iasis:DEI_Prediction
	];
	rr:predicateObjectMap [
		rr:predicate iasis:interactor1;
		rr:objectMap [ 
			rr:template "http://project-iasis.eu/Drug/{Drug}" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:interactor2;
		rr:objectMap [ 
			rr:template "http://project-iasis.eu/Enzyme/{Enzyme}" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:confidence;
		rr:objectMap [ 
			rr:constant "Prob" 
		]
	];	
	rr:predicateObjectMap [
		rr:predicate iasis:provenance;
		rr:objectMap [
			rr:template "http://project-iasis.eu/SourceName/"
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:predictionMethod;
		rr:objectMap [ 
			rr:constant "Method" 
		]
	].	
		
