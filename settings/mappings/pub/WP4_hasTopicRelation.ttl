@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <http://tib.de/ontario/mapping#> .
@prefix iasis: <http://project-iasis.eu/vocab/> .

<#HasTopic>
	rml:logicalSource [ rml:source "/home/jozashoori/External/Data/WP4/has_topic_3.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Relation/{subject}HAS_TOPIC{object}";
		rr:class iasis:HasTopic
	];
	rr:predicateObjectMap [
		rr:predicate iasis:confidence;
		rr:objectMap [ 
			rml:reference "confidence" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:hasAnnotation;
		rr:objectMap [
			rr:template "http://project-iasis.eu/Annotation/{subject}"
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:annotates;
		rr:objectMap [
			rr:template "http://project-iasis.eu/Publication/{object}"
		]
	].

<#Connection>
	rml:logicalSource [ rml:source "/home/jozashoori/External/Data/WP4/has_topic_3.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Publication/{subject}"
	];
	rr:predicateObjectMap [
		rr:predicate iasis:publication_hasTopic_cui;
		rr:objectMap [
			rr:template "http://project-iasis.eu/Annotation/{object}"
		]
	].
