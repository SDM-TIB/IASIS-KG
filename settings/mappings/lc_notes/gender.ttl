@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <http://tib.de/ontario/mapping#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix iasis: <http://project-iasis.eu/vocab/> .

<#Gender>
	rml:logicalSource [ rml:source "/data/data/lc_notes/gender.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Gender/{CUI_ID}";
		rr:class iasis:Gender;
	];
	rr:predicateObjectMap [
		rr:predicate iasis:genderLabel;
		rr:objectMap [ 
			rml:reference "name" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:genderID;
		rr:objectMap [ 
			rml:reference "CUI_ID" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate owl:sameAs;
		rr:objectMap [
			rr:template "{DBpediaURL}";
		]
	].				
