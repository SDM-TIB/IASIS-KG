@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <http://tib.de/ontario/mapping#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix iasis: <http://project-iasis.eu/vocab/> .

<#LCPatient>
	rml:logicalSource [ rml:source "/data/data/lc_notes/patient_stages.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/LCPatient/{patient_id}";
	];
	rr:predicateObjectMap [
		rr:predicate iasis:hasTumorObservation;
		rr:objectMap [ 
			rr:parentTriplesMap <#TumorObservation>
		]
	].	
<#TumorObservation>
	rml:logicalSource [ rml:source "/data/data/lc_notes/patient_stages.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/LCPatient/TumorObservation/{patient_id}_{stage_id}";
		rr:class iasis:TumorObservation;
	];
	rr:predicateObjectMap [
		rr:predicate iasis:hasTumorStage;
		rr:objectMap [ 
			rr:template "http://project-iasis.eu/TumorStage/{CUI}"
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:hasDate;
		rr:objectMap [ 
			rr:template "http://project-iasis.eu/LCPatient/Date/{date}"
		]
	].
