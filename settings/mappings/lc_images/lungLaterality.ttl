@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <http://tib.de/ontario/mapping#> .
@prefix iasis: <http://project-iasis.eu/vocab/> .

<#LungLaterality>
	rml:logicalSource [ rml:source "/data/data/lc_images/lungLaterality.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/LCPatient/LungLaterality/{concept}";
		rr:class iasis:LungLaterality;
	];
	rr:predicateObjectMap [
		rr:predicate iasis:lungLateralityLabel;
		rr:objectMap [
			rml:reference "concept";
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:hasCUIAnnotation;
		rr:objectMap [
			rr:parentTriplesMap <#Annotation>
		]
	].	

<#Annotation>
	rml:logicalSource [ rml:source "/data/data/lc_images/lungLaterality.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Annotation/{cui}";
		rr:class iasis:Annotation
	];
	rr:predicateObjectMap [
		rr:predicate iasis:annID;
		rr:objectMap [ 
			rml:reference "cui" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:provenance;
		rr:objectMap [
			rr:template "http://project-iasis.eu/SourceName/UMLS"
		]
	].	
