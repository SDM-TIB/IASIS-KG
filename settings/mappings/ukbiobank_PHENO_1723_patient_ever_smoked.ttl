@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <http://tib.de/ontario/mapping#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix iasis: <http://project-iasis.eu/vocab/> .

<#DementiaPatient>
	rml:logicalSource [ rml:source "/data/data/dementia/ukbiobank_PHENO_88_patient_ever_smoked.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/DementiaPatient/{patient_id}";
	];
	rr:predicateObjectMap [
		rr:predicate iasis:hasEverSmokedObservation;
		rr:objectMap [
			rr:parentTriplesMap <#EverSmokedObservation>
		]
	].	
			
<#EverSmokedObservation>
	rml:logicalSource [ rml:source "/data/data/dementia/ukbiobank_PHENO_88_patient_ever_smoked.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/EverSmokedObservation/{patient_id}_{peroid}_{ever_smoked}";
		rr:class iasis:EverSmokedObservation
	];			
	rr:predicateObjectMap [
		rr:predicate iasis:everSmokedPeriod;
		rr:objectMap [ 
			rml:reference "peroid" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:existenceOf_everSmoked;
		rr:objectMap [ 
			rml:reference "ever_smoked"
		]
	].