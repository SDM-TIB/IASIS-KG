
@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <http://tib.de/ontario/mapping#> .
@prefix iasis: <http://project-iasis.eu/vocab/> .


<#LCPatient>
	rml:logicalSource [ rml:source "/data/data/WP2/ecog_wp2-1523023412772.csv";
						rml:referenceFormulation ql:CSV
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/LCPatient/{id_patient}"
	];
	rr:predicateObjectMap [
		rr:predicate iasis:hasEcog;
		rr:objectMap [ 
			rr:parentTriplesMap <#Ecog> 
		]
	].

<#Ecog>
	rml:logicalSource [ rml:source "/data/data/WP2/ecog_wp2-1523023412772.csv";
						rml:referenceFormulation ql:CSV
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Ecog/{id}";
		rr:class iasis:Ecog
	];
	rr:predicateObjectMap [
		rr:predicate iasis:hasObservation;
		rr:objectMap [ 
			rr:parentTriplesMap <#EcogObservation>
		]
	].					

<#EcogObservation>	
	rml:logicalSource [ rml:source "/data/data/WP2/ecog_wp2-1523023412772.csv";
						rml:referenceFormulation ql:CSV
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/EcogObservation/{id_patient}_{id}";
		rr:class iasis:EcogObservation
	];
	rr:predicateObjectMap [
		rr:predicate iasis:identifier;
		rr:objectMap [ 
			rml:reference "id"
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:value;
		rr:objectMap [
			rml:reference "ecog"
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:date;
		rr:objectMap [
			rml:reference "date"
		]
	].