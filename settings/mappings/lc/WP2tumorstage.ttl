@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <http://tib.de/ontario/mapping#> .
@prefix iasis: <http://project-iasis.eu/vocab/> .

<#LCPatient>
	rml:logicalSource [ rml:source "/data/data/lc/stage_wp2-1523023412772.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/LCPatient/{id_patient}";
	];
	rr:predicateObjectMap [
		rr:predicate iasis:hasTumorStage;
		rr:objectMap [ 
			rr:parentTriplesMap <#TumorStage> 
		]
	].

<#TumorStage>
	rml:logicalSource [ rml:source "/data/data/lc/stage_wp2-1523023412772.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/TumorStage/{id}";
		rr:class iasis:TumorStage
	];	
	rr:predicateObjectMap [
		rr:predicate iasis:hasObservation;
		rr:objectMap [ 
			rr:parentTriplesMap <#TumorStageObservation> 
		]
	].

<#TumorStageObservation>

	rml:logicalSource [ rml:source "/data/data/lc/stage_wp2-1523023412772.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/TumorStageObservation/{id_patient}_{id}";
		rr:class iasis:TumorStageObservation
	];		
	rr:predicateObjectMap [
		rr:predicate iasis:identifier;
		rr:objectMap [ 
			rml:reference "id"
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:tumorStage;
		rr:objectMap [ 
			rml:reference "tumorStage"
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:date;
		rr:objectMap [ 
			rml:reference "date"
		]
	].
