@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <http://tib.de/ontario/mapping#> .
@prefix iasis: <http://project-iasis.eu/vocab/> .

<#TumorType>
	rml:logicalSource [ rml:source "/home/jozashoori/External/Data/Deliverable/Clinical/TumorType.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/TumorType/{t_concept}";
		rr:class iasis:TumorType;
	];
	rr:predicateObjectMap [
		rr:predicate iasis:lcType;
		rr:objectMap [
			rml:reference "type"
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:hasAnnotation;
		rr:objectMap [
			rr:parentTriplesMap <#TumorType_Annotation>
		]
	].				

<#TumorType_Annotation>
	rml:logicalSource [ rml:source "/home/jozashoori/External/Data/Deliverable/Clinical/TumorType.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Annotation/{t_cui}";
		rr:class iasis:Annotation
	];
	rr:predicateObjectMap [
		rr:predicate iasis:annID;
		rr:objectMap [ 
			rml:reference "t_cui" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:annLabel;
		rr:objectMap [ 
			rml:reference "t_concept" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:provenance;
		rr:objectMap [
			rr:template "http://project-iasis.eu/SourceName/UMLS"
		]
	].	