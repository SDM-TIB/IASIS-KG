@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <http://tib.de/ontario/mapping#> .
@prefix iasis: <http://project-iasis.eu/vocab/> .

<#LCPatient>
	rml:logicalSource [ rml:source "/app/Data/WP2/biopsia_wp2-1523023412772.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/LCPatient/{id_patient}";
	];
	rr:objectMap [
		rr:predicate iasis:hasBiopsy;
		rr:objectMap [ 
			rr:parentTriplesMap <#LiquidBiopsy> 
		]
	].

<#LiquidBiopsy>
	rml:logicalSource [ rml:source "/app/Data/WP2/biopsia_wp2-1523023412772.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/LiquidBiopsy/{id}";
		rr:class iasis:LiquidBiopsy
	];
	rr:objectMap [
		rr:predicate iasis:hasObservation;
		rr:objectMap [ 
			rr:parentTriplesMap <#LiquidBiopsyObservation> 
		]
	].

<#LiquidBiopsyObservation>
	rml:logicalSource [ rml:source "/app/Data/WP2/biopsia_wp2-1523023412772.csv";
						rml:referenceFormulation ql:CSV;
						]; 
	rr:subjectMap [
		rr:template "http://project-iasis.eu/LiquidBiopsyObservation/{id_patient}_{id}";
		rr:class iasis:LiquidBiopsyObservation
	];	
	rr:predicateObjectMap [
		rr:predicate iasis:identifier;
		rr:objectMap [ 
			rml:reference "id";
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:sampleDate;
		rr:objectMap [ 
			rml:reference "sampleDate";
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:resultDate;
		rr:objectMap [ 
			rml:reference "resultsDate";
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:mutationName;
		rr:objectMap [ 
			rml:reference "mutation"; 
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:targetTotal;
		rr:objectMap [ 
			rml:reference "targetTotal";
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:copiesMicroliterFam;
		rr:objectMap [ 
			rml:reference "copiesMicroliterFam";
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:copiesMicroliterVic;
		rr:objectMap [ 
			rml:reference "copiesMicroliterVic";
		]
	].	