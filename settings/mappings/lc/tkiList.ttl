@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <http://tib.de/ontario/mapping#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix iasis: <http://project-iasis.eu/vocab/> .

<#TkiDrug>
	rml:logicalSource [ rml:source "/home/jozashoori/External/Data/Deliverable/data/lc/TkiList.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/TkiDrug/{tki_concept}";
		rr:class iasis:TkiDrug;
	];
	rr:predicateObjectMap [
		rr:predicate iasis:hasAnnotation;
		rr:objectMap [
			rr:parentTriplesMap <#TkiDrug_Annotation>
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:tkiDrugLabel;
		rr:objectMap [ 
			rml:reference "tki_concept" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate owl:sameAs;
		rr:objectMap [
			rr:template "{Dbpedia}";
		]
	].				

<#TkiDrug_Annotation>
	rml:logicalSource [ rml:source "/home/jozashoori/External/Data/Deliverable/data/lc/TkiList.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Annotation/{tki_cui}";
		rr:class iasis:Annotation
	];
	rr:predicateObjectMap [
		rr:predicate iasis:annID;
		rr:objectMap [ 
			rml:reference "tki_cui" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:provenance;
		rr:objectMap [
			rr:template "http://project-iasis.eu/SourceName/UMLS"
		]
	].
